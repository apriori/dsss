profile=phobos

compiler=dmd
inifile=sc.ini

exeext=.exe
objext=obj


version=DigitalMars
noversion=GNU
noversion=linux
noversion=Unix
noversion=Posix
version=Windows
version=X86
noversion=PPC
noversion=X86_64
version=LittleEndian
noversion=BigEndian


[compile]
cmd=dmd -c $i -of$o

flag=$i
incdir=-I$i
libdir=-L-L$i
optimize=-O
version=-version=$i


[link]
cmd=dmd $i -of$o

libdir=-L$i
lib=-L$i.lib
flag=-L$i


[liblink]
cmd=lib -c $o $i

libdir=
lib=
flag=


[postliblink]
cmd=ranlib $i


[shliblink]
shlibs=no
