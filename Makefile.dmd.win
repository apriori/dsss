PREFIX=C:\\dsss
MAKE=make

all: dsss.exe

dsss.exe: dsss_build/dsss_build.exe
	dsss_build\\dsss_build.exe -exec getmango.d
	dsss_build\\dsss_build.exe -od. -Idsss_build sss\\main.d -Tdsss
	dsss.exe build

dsss_build\\dsss_build.exe:
	cd dsss_build && \
	$(MAKE) -fMakefile.dos

install: dsss.exe
	dsss.exe install --prefix=$(PREFIX)

clean:
	-dsss.exe distclean
	del *.obj
	del dsss.exe
	cd dsss_build && \
	$(MAKE) -f Makefile.dos clean
