profile=phobos

compiler=dmd

exeext=
objext=o


[compile]
cmd=dmd -version=Posix -c $i -of$o

flag=$i
incdir=-I$i
libdir=-L-L$i
optimize=-O
version=-version=$i


[link]
cmd=dmd $i -of$o

libdir=-L-L$i
lib=-L-l$i
flag=-L$i


[liblink]
cmd=ar rc $o $i

libdir=
lib=
flag=


[postliblink]
cmd=ranlib $i


[shliblink]
shlibs=no
