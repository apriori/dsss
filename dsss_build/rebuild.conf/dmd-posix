profile=phobos

exeext=
objext=o

compiler=dmd

compile=dmd -c $i -of$o
incflag=-I$i
compile_libflag=-L-L$i
compile_flag=$i
optimizeflag=-O
versionflag=-version=$i

link=dmd $i -of$o
link_libflag=-L-L$i
link_lib=-L-l$i
link_flag=-L$i

liblink=ar rc $o $i
liblink_libdir=
liblink_lib=
liblink_flag=

postliblink=ranlib $i

shlibs=no
